{
  "name": "@tiny-codes/eslint-config",
  "version": "1.0.0",
  "description": "Provides most common used eslint, prettier, stylelint, prettier configurations, ALL IN ONE.",
  "keywords": [
    "code standard",
    "eslint",
    "prettier",
    "husky",
    "commitlint",
    "shared config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shijistar/eslint-config"
  },
  "author": "李凤宝(Fabio) <shijistar@gmail.com>",
  "main": "index.js",
  "files": [
    "src",
    "index.js",
    "CHANGELOG.md",
    "README.md",
    "tsconfig.json",
    "tsconfig.eslint.json"
  ],
  "scripts": {
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix",
    "*.{css,less,scss}": "stylelint --fix",
    "*.{js,jsx,ts,tsx,vue,md,json,css,less,scss}": "prettier --write"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.24.6",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tanstack/eslint-plugin-query": "^5.35.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint": "^8.56.10",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "commitlint": "^19.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.1",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-packagejson": "^2.5.0",
    "prettier-plugin-two-style-order": "^1.0.1",
    "stylelint": "^16.6.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "conventional-changelog-cli": "^5.0.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "prettier": ">=3.2.5",
    "stylelint": ">=16.1.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.2.0",
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.2.0"
  }
}
